<!DOCTYPE html>
<html>
  <head>
    <title>HeadStartTta</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <main id="main-content">
      <div class="grid-container">
        <div class="grid-row">
          <div class="grid-col">
            <%= yield %>
          </div>
        </div>
      </div>
    </main>
  </body>
</html>
